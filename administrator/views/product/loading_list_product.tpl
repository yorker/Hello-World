<{if !empty($list)}><table class="adminlist" cellspacing="1">	<thead>		<tr>			<th width="30"><input type="checkbox" onclick="checkAll(this, 'chkflag')" title="<{$lang.check_all_none}>"/></th>			<th><{$lang.name}></th>			<th width="30"><{$lang.image}></th>			<th width="100"><{$lang.belong_cat}></th>			<th width="30"><{$lang.is_best}></th>			<th width="30"><{$lang.is_new}></th>			<th width="30"><{$lang.is_hot}></th>			<th width="60"><{$lang.ordering}></th>			<th width="30"><{$lang.published}></th>			<th width="100"><{$lang.operation}></th>		</tr>	</thead>	<tbody>		<{foreach from=$list item="itm" name="fname"}>		<tr class="row<{$smarty.foreach.fname.iteration%2}>">			<td align="center"><input type="checkbox" name="cid" class="chkflag" value="<{$itm.product_id}>"/></td>			<td>				<{$itm.product_name}>				<{if $itm.product_thumb>''}><img title="" src="images/picflag.gif" border="0" onmouseover="showImg('<{$smarty.const.UPLOAD_URL}><{$itm.product_thumb}>')" onmouseout="nd()"/><{/if}>			</td>			<td align="center"><{$itm.gallery_num}></td>			<td align="center"><{$itm.cat_name}></td>			<td align="center"><{admin_switcher t="products" f="is_best" value=$itm.is_best id=$itm.product_id}></td>			<td align="center"><{admin_switcher t="products" f="is_new" value=$itm.is_new id=$itm.product_id}></td>			<td align="center"><{admin_switcher t="products" f="is_hot" value=$itm.is_hot id=$itm.product_id}></td>			<td align="center"><div class="dyna_edit" onclick="loadEdit(this, 'products', 'ordering', '<{$itm.product_id}>', 1, 's')"><{$itm.ordering}></div></td>			<td align="center"><{admin_switcher t="products" f="published" value=$itm.published id=$itm.product_id}></td>			<td align="center">				<{admin_edit href="product/writeProduct"|app_url id=$itm.product_id}>				<{admin_trash t="products" f="is_deleted" v="1" id=$itm.product_id url=$here is_page='1'}>				<{admin_delete t="products" id=$itm.product_id url="product/listProduct" image="product_img" req_url="product/procDeleteProduct"|app_url}>			</td>		</tr>		<{/foreach}>	</tbody>	<tfoot>		<tr>			<td colspan="10">				<div class="clearfix">					<div class="fl">						<input type="button" value="<{$lang.delete}>" class="btn" onclick="batchDeleteByCheckbox('products', 'chkflag', '<{app_url url='product/listProduct'}>', '', 'product_img', '<{app_url url='product/procDeleteProduct'}>')"/>&nbsp;						<input type="button" value="<{$lang.trash}>" class="btn" onclick="batchUpdateByCheckbox('products', 'is_deleted', '1', 'chkflag', '<{app_url url='product/listProduct'}>')"/>						&nbsp;						<input type="button" value="<{$lang.move_to}>" class="btn" onclick="if($('.chkflag:checked').length>0) openAjaxWin('<{app_url url='product/listProduct?act=batch_move'}>', 300, 180); else alert('<{$lang.please_choose_the_operation_option}>');"/>					</div>					<div class="fr"><{$pages}></div>				</div>			</td>		</tr>	</tfoot>	</table><{else}>	<div class="no_data"><{$lang.no_data}></div><{/if}>