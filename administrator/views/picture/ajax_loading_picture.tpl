<{if !empty($list)}><table class="adminlist" cellspacing="1">	<thead>		<tr>			<th width="20"><input type="checkbox" onclick="checkAll(this, 'chkflag')"/></th>			<th width="25"><{$lang.id}></th>			<th width="122"><{$lang.cover_img}></th>			<th><{$lang.title}></th>			<th width="122"><{$lang.create_time}></th>			<th width="30"><{$lang.amount}></th>			<th width="30"><{$lang.published}></th>			<th width="70"><{$lang.ordering}></th>			<th width="80"><{$lang.operation}></th>		</tr>	</thead>	<tbody>		<{foreach from=$list item="itm" name="fname"}>		<tr class="row<{$smarty.foreach.fname.iteraction%2}>">			<td align="center"><input type="checkbox" name="cid" value="<{$itm.id}>" class="chkflag"/></td>			<td align="center"><{$itm.id}></td>			<td align="center"><img src="<{$upload_url}><{$itm.cover_img_thumb}>" border="0"/></td>			<td><{$itm.title}></td>			<td align="center"><{$itm.create_datetime|date_format:'%Y-%m-%d'}></td>			<td align="center"><{$itm.gallery_num}></td>			<td align="center"><{admin_switcher t="picture_collect" f="published" value=$itm.published id=$itm.id}></td>			<td align="center"><div class="dyna_edit" onclick="loadEdit(this, 'picture_collect', 'ordering', '<{$itm.id}>', '1')"><{$itm.ordering}></div></td>			<td align="center">				<{if $cat_id > 0}>					<{admin_edit href=$cat_id|pcat:"picture/writePicture?cat_id="|app_url id=$itm.id}>				<{else}>					<{admin_edit href="picture/writePicture"|app_url id=$itm.id}>				<{/if}>&nbsp;				<a href="javascript:void(0)" onclick="delete_pic_gallery('<{$itm.id}>', '<{if $cat_id>0}><{app_url url='picture/listPicture?cat_id='|cat:$cat_id}><{else}><{app_url url='picture/listPicture'}><{/if}>')"><img src="images/icon_drop.gif" border="0"/></a>			</td>		</tr>		<{/foreach}>	</tbody>	<tfoot>		<tr>			<td colspan="9">				<div class="clearfix">					<div class="fl"><input type="button" name="batch_delete_picture" value="<{$lang.batch_delete}>" onclick="delete_pic_gallery('chkflag', '<{if $cat_id>0}><{app_url url='picture/listPicture?cat_id='|cat:$cat_id}><{else}><{app_url url='picture/listPicture'}><{/if}>')" class="btn"/></div>					<div class="fr"><{$pages}></div>				</div>			</td>		</tr>	</tfoot></table><{else}>	<div class="no_data"><{$lang.no_data}></div><{/if}>